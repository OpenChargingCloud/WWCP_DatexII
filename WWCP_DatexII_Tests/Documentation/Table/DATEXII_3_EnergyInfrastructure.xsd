  <xs:complexType name="EnergyInfrastructureSite">
    <xs:annotation>
      <xs:documentation>A site where vehicles can be supplied with energy, including all buildings, stations, parking spaces and other associated services.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="fac:Facility">
        <xs:sequence>
          <xs:element name="typeOfSite" type="egi:_EnergyInfrastructureSiteTypeEnum" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Specifies the type of the site.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="brand" type="com:MultilingualString" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>The brand of the site.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="exclusiveUsers" type="fac:_UserTypeEnum" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Limitation to a set of users (exclusive)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="preferredUsers" type="fac:_UserTypeEnum" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Users that are preferred at this site (but not exclusive)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="serviceType" type="egi:ServiceType" minOccurs="0" maxOccurs="unbounded">
