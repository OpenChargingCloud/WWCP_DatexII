          <xs:documentation>Three-wheel moped.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="l3">
        <xs:annotation>
          <xs:documentation>Two-wheel motorcycle.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="l4">
